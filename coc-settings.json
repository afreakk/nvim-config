{
    "yaml.trace.server": "verbose",
    "yaml.format.enable": true,
    "yaml.schemas": {
        "https://json.schemastore.org/kustomization.json": [
            "kustomization.yaml",
            "kustomization.yml"
        ]
    },
    "yaml.schemaStore.enable": true,
    "codeLens.enable": true,
    "diagnostic.errorSign": "",
    "diagnostic.warningSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.virtualText": true,
    "diagnostic.hintSign": "",
    "omnisharp.trace.server": "verbose",
    "omnisharp.path": "omnisharp",
    "explorer.icon.enableNerdfont": true,
    "explorer.previewAction.onHover": false,
    "explorer.file.reveal.auto": true,
    "prettier.tabWidth": 4,
    "prettier.singleQuote": true,
    "cSpell.allowCompoundWords": true,
    "cSpell.dictionaryDefinitions": [
        { "name": "norsk", "path": "/home/afreak/norsk.txt" }
    ],
    "cSpell.dictionaries": ["norsk"],
    "diagnostic-languageserver.filetypes": {
        "sh": "shellcheck",
        "markdown": ["write-good", "markdownlint"]
    },
    "diagnostic-languageserver.formatFiletypes": {
        "starlark": "buildifier"
    },
    "diagnostic-languageserver.formatters": {
        "buildifier": {
            "command": "buildifier"
        }
    },
    "coc.preferences.formatOnSaveFiletypes": [
        "starlark",
        "javascript",
        "typescript",
        "typescriptreact",
        "json",
        "javascriptreact",
        "less",
        "svelte",
        "rust",
        "elm"
    ],
    "tsserver.trace.server": "verbose",
    "languageserver": {
        "graphql": {
            "command": "graphql-lsp",
            "args": ["server", "-m", "stream"],
            "filetypes": ["graphql"]
        },
        "nix": {
            "command": "rnix-lsp",
            "filetypes": ["nix"]
        },
        "terraform": {
            "command": "/nix/store/6hn07zrp44srvgxp9jys4h903lcrbqmm-terraform-ls-0.18.1/bin/terraform-ls",
            "filetypes": ["terraform", "tf"]
        },
        "haskell": {
            "command": "haskell-language-server-wrapper",
            "args": ["--lsp"],
            "rootPatterns": [
                "*.cabal",
                "stack.yaml",
                "cabal.project",
                "package.yaml"
            ],
            "filetypes": ["hs", "lhs", "haskell"],
            "initializationOptions": {
                "languageServerHaskell": {}
            }
        },
        "elmLS": {
            "command": "elm-language-server",
            "filetypes": ["elm"],
            "rootPatterns": ["elm.json"],
            "initializationOptions": {
                "elmAnalyseTrigger": "change"
            }
        }
    },
    "cSpell.userWords": [
      "Dgraph",
      "Ebillett",
      "Groot",
      "OIDC",
      "Optidrive",
      "Steptronic",
      "TLDR",
      "Uids",
      "Xdebug",
      "afreak",
      "argoproj",
      "basicauth",
      "bson",
      "buildifier",
      "clangd",
      "cloudinary",
      "crds",
      "dxcn",
      "dxdx",
      "dxweb",
      "dxwebserver",
      "efestival",
      "eksctl",
      "envvars",
      "fastify",
      "firstname",
      "gosm",
      "higlight",
      "ixpartner",
      "ixusers",
      "konfigurasjon",
      "kratos",
      "kustomization",
      "kustomize",
      "lastname",
      "ledgerno",
      "mamut",
      "mbapi",
      "mkdir",
      "mutators",
      "ollivanders",
      "omnisharp",
      "organisasjonsnummer",
      "oryd",
      "partneradmin",
      "phpls",
      "pivetdrive",
      "postnr",
      "rnix",
      "strongdm",
      "upsert"
    ]
}
